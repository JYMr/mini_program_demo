/* pages/Common/Dialog.wxss */
.Dialog{width: 465rpx; background: #FFF;position: fixed;top: 50%;left: 0;right: 0;margin:auto;
	border-radius: 10rpx;transform: translate(0, -50%);z-index: 999;animation-name: DialogfadeInDown;animation-duration: .3s;animation-fill-mode: both;}

.Dialog-text{line-height: 42rpx;padding: 65rpx 75rpx;text-align: center;}
.Dialog-btn{height: 88rpx;display: flex;line-height: 88rpx;text-align: center;border-top: 1rpx solid #ddd;}
.Dialog-btn .btn-item{flex: 1;box-sizing: border-box;}
.Dialog-btn .btn-item.confirm{color: #37c4a4;}
.Dialog-btn .btn-item + .btn-item{border-left: 1rpx solid #ddd;}

.Dialog-1{padding: 52rpx 0;text-align: center;}
.Dialog-1 .icon{width: 36rpx; height: 36rpx;vertical-align: bottom;margin-right: 15rpx;display: inline-block;}
.Dialog-1 .icon.icon-success{background: url(data:image/png;base64,) center center no-repeat;background-size: 36rpx;}
.Dialog-1 .icon.icon-fail{background: url(data:image/png;base64,) center center no-repeat;background-size: 36rpx;}
.Dialog-1 text{line-height: 36rpx;display: inline-block;}

.mask{position: fixed;z-index: 998;top: 0;bottom: 0;left: 0;right: 0;margin: auto; background: rgba(0, 0, 0, 0.5);}

@keyframes DialogfadeInDown{from{transform: translate3d(0, -80%, 0);opacity: 0;}
    to{transform: translate(0, -50%);opacity: 1;}}